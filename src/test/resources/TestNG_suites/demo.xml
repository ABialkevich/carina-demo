<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite verbose="1" name="Carina Demo Tests"
	skipfailedinvocationcounts="false" junit="false" parallel="tests"
	data-provider-thread-count="50" annotations="JDK">

	<test name="gsmarenaTests">
		<parameter name="tableName" value="launch"/>
		<parameter name="rowName" value="announced"/>
		<classes>
			<class name="com.qaprosoft.carina.core.demo.GsmArenaTests"/>
		</classes>
	</test>

	<test name="apiTests">
		<classes>
			<class name="com.qaprosoft.carina.core.demo.UserTests"/>
		</classes>
	</test>
</suite>