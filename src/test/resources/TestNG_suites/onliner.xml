<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite verbose="1" name="Onliner tests"
	skipfailedinvocationcounts="false" junit="false" parallel="tests"
	data-provider-thread-count="50" annotations="JDK"> 

	<test verbose="1" name="Onliner - valid login test">
	    
	    <parameter name="email" value="alex@onflydev.com"/>
	    <parameter name="password" value="test2016"/>
	    <parameter name="username" value="1924336"/>
	    
		<classes>
			<class name="com.qaprosoft.carina.core.demo.OnlinerTest">
				<methods>
					<include name="testOnlinerLogin" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test verbose="1" name="Onliner - invalid login test">
	    
	    <parameter name="email" value="alex@onflydev.com"/>
	    <parameter name="password" value="123456"/>
	    <parameter name="username" value="1924336"/>
	    
		<classes>
			<class name="com.qaprosoft.carina.core.demo.OnlinerTest">
				<methods>
					<include name="testOnlinerLogin" />
				</methods>
			</class>
		</classes>
	</test>
	
</suite>